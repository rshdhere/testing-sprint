generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Requests{
  id     Int          @id @default(autoincrement())
  type   RequestType
  a      Int
  b      Int
  answer Int
}

enum RequestType{
  Sum
  Multiply
}
